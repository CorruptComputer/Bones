{
  "Serilog": {
    "Using":  [ "Serilog.Sinks.Console" ],
    "MinimumLevel": "Warning",
    "WriteTo": [
      { "Name": "Console" }
    ],
    "Enrich": [ "FromLogContext" ]
  },
  "Database": {
    "UseInMemoryDb": true
  },
  "Logging": {
    "LogLevel": {
      "Default": "Warning"
    }
  }
}
